{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "bigint-hash": "^0.2.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "fs-extra": "^8.0.1",
    "jszip": "^3.2.1",
    "morgan": "~1.9.0",
    "osmosis": "^1.1.10",
    "redis": "^2.8.0",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.0"
  }
}
